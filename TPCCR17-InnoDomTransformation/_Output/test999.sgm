                                                                                                                                                                                                      <TBLCDEF COLWD="25" TBLUNITS="PERCENT" />
                                                                                                                                                                                                    </TBLCDEFS>
                                                                                                                                                                                                    <TBLROWS ROWSEP="HSINGLE" VALIGN="TOP" LEFTSEP="VSINGLE">
                                                                                                                                                                                                      <TBLROW>
                                                                                                                                                                                                        <TBLCELL ROWSEP="HSINGLE" COLSTART="1">
                                                                                                                                                                                                          <BOLD>Focus Area</BOLD>
                                                                                                                                                                                                        </TBLCELL>
                                                                                                                                                                                                        <TBLCELL ROWSEP="HSINGLE" COLSTART="2">
                                                                                                                                                                                                          <BOLD>Observations</BOLD>
                                                                                                                                                                                                        </TBLCELL>
                                                                                                                                                                                                        <TBLCELL ROWSEP="HSINGLE" COLSTART="3"></TBLCELL>
                                                                                                                                                                                                        <TBLCELL ROWSEP="HSINGLE" COLSTART="4">
                                                                                                                                                                                                          <BOLD>Considerations</BOLD>
                                                                                                                                                                                                        </TBLCELL>
